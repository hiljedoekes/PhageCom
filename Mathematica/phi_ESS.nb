Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"sol", " ", "=", " ", 
  RowBox[{"Assuming", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"P0", ">", "0"}], ",", " ", 
      RowBox[{"\[Phi]", "<", 
       RowBox[{"1", "-", 
        RowBox[{"\[Beta]", "/", "a"}]}]}], ",", " ", 
      RowBox[{"\[Beta]", ">", "a"}], ",", " ", 
      RowBox[{"a", ">", "0"}]}], "}"}], ",", " ", 
    RowBox[{"Simplify", "[", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"J", "[", 
         RowBox[{"T", ",", "\[Phi]", ",", "P0"}], "]"}], "\[Equal]", "1"}], 
       ",", "T", ",", " ", "Reals"}], "]"}], "]"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tinf", "[", 
   RowBox[{"\[Phi]_", ",", "P0_"}], "]"}], " ", "=", " ", 
  RowBox[{"T", "/.", 
   RowBox[{"sol", "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]}], "Input",
 CellChangeTimes->{{3.784455206085638*^9, 3.784455207788726*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"T", "\[Rule]", 
    FractionBox[
     RowBox[{"Log", "[", 
