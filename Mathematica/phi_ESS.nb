Cell["\<\
Making further simplifying assumptions, we can get an analytical expression \
for \[Phi] at ESS\
\>", "Text",
 CellChangeTimes->{{3.7584611098486433`*^9, 3.7584611295401907`*^9}},
 Background->RGBColor[1, 1, 0.85]],

Cell["\<\
Getting an analytical expression for \[Phi]* was impossible because we could \
not solve the root of m[t,\[Phi],P0] exactly. However, we can use knowledge \
of the parameter values to get a pretty good approximation of m (see \
supporting text S3):\
\>", "Text",
 CellChangeTimes->{{3.7584611567707243`*^9, 3.758461274638888*^9}, {
  3.7584613752089853`*^9, 3.7584613769236507`*^9}, {3.758461494033996*^9, 
  3.758461504905533*^9}, {3.7844564030612783`*^9, 3.784456416556438*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"mapprox", "[", 
   RowBox[{"t_", ",", "\[Phi]_", ",", "P0_"}], "]"}], " ", "=", " ", 
  RowBox[{"\[Beta]", " ", "P0", " ", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Beta]", 
         RowBox[{"(", 
          RowBox[{"1", "-", "\[Phi]"}], ")"}]}], "-", "a"}], ")"}], " ", 
      "t"}], "]"}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"\[Beta]", 
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Phi]"}], ")"}]}], "-", "a"}], ")"}]}], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Beta]", "-", "a"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Beta]", 
         RowBox[{"(", 
          RowBox[{"1", "-", "\[Phi]"}], ")"}]}], "-", "a"}], ")"}]}], " ", 
     "-", " ", 
     RowBox[{"\[Beta]", " ", "t", " ", "\[Phi]"}]}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.758461253513538*^9, 3.758461279655332*^9}, {
  3.758461390489452*^9, 3.758461394736875*^9}, {3.75846153937222*^9, 
  3.758461609468082*^9}, {3.758461645757547*^9, 3.7584617132133837`*^9}, {
  3.758461828480733*^9, 3.7584618395993958`*^9}, {3.784456428782797*^9, 
  3.784456480040132*^9}, {3.784456519081805*^9, 3.784456522232189*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"t", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "a"}], "+", 
       RowBox[{"\[Beta]", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Phi]"}], ")"}]}]}], ")"}]}]], " ", "P0", " ", 
   "\[Beta]", " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"-", "a"}], "+", "\[Beta]"}], 
      RowBox[{
       RowBox[{"-", "a"}], "+", 
       RowBox[{"\[Beta]", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Phi]"}], ")"}]}]}]], "-", 
     RowBox[{"t", " ", "\[Beta]", " ", "\[Phi]"}]}], ")"}]}], 
  RowBox[{
   RowBox[{"-", "a"}], "+", 
   RowBox[{"\[Beta]", " ", 
    RowBox[{"(", 
     RowBox[{"1", "-", "\[Phi]"}], ")"}]}]}]]], "Output",
