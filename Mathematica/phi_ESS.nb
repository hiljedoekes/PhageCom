     SuperscriptBox[
      RowBox[{"Log", "[", 
       RowBox[{"1", "+", 
        FractionBox["1", "P0"], "-", 
        FractionBox["a", 
         RowBox[{"P0", " ", "\[Beta]"}]]}], "]"}], "2"]}]], "+", 
