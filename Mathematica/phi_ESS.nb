Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"PTinf", "[", 
   RowBox[{"\[Phi]_", ",", "P0_"}], "]"}], "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"P0", " ", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "\[Phi]"}], ")"}], "\[Beta]"}], "-", "a"}], ")"}], 
      RowBox[{"Tinf", "[", 
       RowBox[{"\[Phi]", ",", "P0"}], "]"}]}], "]"}]}], "]"}]}]], "Input",
