  1, 1, 0.85],ExpressionUUID->"45d416be-8a9e-4a38-8376-2065e254b907"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"l", "[", 
   RowBox[{"t_", ",", "\[Phi]_"}], "]"}], "=", " ", 
  RowBox[{
   RowBox[{"(", " ", 
    RowBox[{"\[Beta]", " ", "P0", " ", 
     RowBox[{"\[Phi]", " ", "/", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
