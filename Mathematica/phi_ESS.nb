Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", "-", "\[Beta]"}], ")"}], " ", "\[Phi]"}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", "-", "\[Beta]", "-", 
      RowBox[{"P0", " ", "\[Beta]"}]}], ")"}], " ", 
    SuperscriptBox[
     RowBox[{"Log", "[", 
