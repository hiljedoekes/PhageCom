Cell[BoxData[
 RowBox[{
  RowBox[{"J", "[", 
   RowBox[{"t_", ",", "\[Phi]_", ",", "P0_"}], "]"}], " ", "=", " ", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"\[Beta]", " ", "P0", "  ", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", "\[Phi]"}], ")"}], "\[Beta]"}], "-", "a"}], 
        ")"}], "x"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "t"}], "}"}]}], "]"}]}]], "Input",
